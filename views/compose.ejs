<%- include("./partials/header") %>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="titleInput">Title</label>
            <input type="text" name="postTitle" id="titleInput" class="form-control">
        </div>
        <div class="form-group">
            <label for="contentInput">Post</label>
            <textarea name="postContent" id="contentInput" cols="30" rows="8" class="form-control"></textarea>
        </div>

        <button type="submit" name="button" class="btn btn-primary">Publish</button>
    </form>
    <%- include("./partials/footer") %>